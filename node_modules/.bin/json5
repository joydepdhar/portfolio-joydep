#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/joydep/Python-Development-Project/Personal-Project-for-Me/portfolio-joydep/node_modules/json5/lib/node_modules:/home/joydep/Python-Development-Project/Personal-Project-for-Me/portfolio-joydep/node_modules/json5/node_modules:/home/joydep/Python-Development-Project/Personal-Project-for-Me/portfolio-joydep/node_modules:/home/joydep/Python-Development-Project/Personal-Project-for-Me/node_modules:/home/joydep/Python-Development-Project/node_modules:/home/joydep/node_modules:/home/node_modules:/node_modules:/home/joydep/Python-Development-Project/Personal-Project-for-Me/portfolio-joydep/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/joydep/Python-Development-Project/Personal-Project-for-Me/portfolio-joydep/node_modules/json5/lib/node_modules:/home/joydep/Python-Development-Project/Personal-Project-for-Me/portfolio-joydep/node_modules/json5/node_modules:/home/joydep/Python-Development-Project/Personal-Project-for-Me/portfolio-joydep/node_modules:/home/joydep/Python-Development-Project/Personal-Project-for-Me/node_modules:/home/joydep/Python-Development-Project/node_modules:/home/joydep/node_modules:/home/node_modules:/node_modules:/home/joydep/Python-Development-Project/Personal-Project-for-Me/portfolio-joydep/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../json5/lib/cli.js" "$@"
else
  exec node  "$basedir/../json5/lib/cli.js" "$@"
fi
